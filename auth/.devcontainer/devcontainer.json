{
	"name": "Go Auth Server DevContainer",
	"dockerComposeFile": ["../../docker-compose.yml"],
	"service": "auth",
	"workspaceFolder": "/workspace/auth",
	"customizations": {
		"vscode": {
			"extensions": [
				"golang.go",
				"ms-azuretools.vscode-docker"
			]
		}
	},
	"runArgs": [
		"--init", 
		"--privileged"
	],
	"features": {
		"ghcr.io/devcontainers/features/docker-in-docker:2": {}
	}
}