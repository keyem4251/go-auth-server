{
	"name": "Go Auth Server DevContainer",
	"dockerComposeFile": ["../../docker-compose.yml"],
	"service": "auth",
	"workspaceFolder": "/workspace/auth",
	"customizations": {
		"vscode": {
			"extensions": [
				"golang.go",
				"ms-azuretools.vscode-docker"
			]
		}
	},
	"runArgs": [
		"--privileged"
	],
	"settings": {
    	"terminal.integrated.shell.linux": "/bin/sh"
	},
	"remoteUser": "root"
}